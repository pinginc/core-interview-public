{
    "name": "core-interview-public",
    "version": "1.0.0",
    "description": "Time By Ping's interview exercises",
    "author": "Time By Ping, Inc.",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:docker": "docker-compose up; docker-compose logs",
        "start:mongo": "docker-compose up -d mongo",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --config=jest.config.ts",
        "test:watch": "jest --config=jest.config.ts --watch",
        "test:e2e": "jest --config=jest.config.e2e.ts",
        "test:e2e:watch": "jest --config=jest.config.e2e.ts --watch"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "deep-equal": "^2.0.5",
        "dotenv": "^16.0.2",
        "mongodb": "^4.9.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.5.0",
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
